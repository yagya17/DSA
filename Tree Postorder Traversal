class Solution {
public:
      vector<int>ans;
    
    void pre(TreeNode* root)
    {
        if(!root) return;
        
        pre(root->left);
        
        pre(root->right);
        ans.push_back(root->val);
        
    }
    vector<int> postorderTraversal(TreeNode* root) {
           pre(root);
        
        return ans;
    }
};
