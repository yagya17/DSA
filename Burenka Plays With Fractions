#include <bits/stdc++.h>
#include<string.h>
#include <math.h>
#define ll long long int
using namespace std;

int main()
{ 
   int t;
   cin>>t;
   while(t--)
   {
   long long a,b,c,d;cin>>a>>b>>c>>d;int ans=0;
  
   long long k=a*d;
   long long m=b*c;
   
   if(k==m) cout<<0<<endl;
   else if(k==1||m==1){cout<<1<<endl;}
   else  if(k==0||m==0) cout<<1<<endl;
   else if(k%m==0||m%k==0) {cout<<1<<endl;}
   else cout<<2<<endl;
   
   }
   
}
